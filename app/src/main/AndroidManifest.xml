        <activity android:name=".module.ryg.ch2.provider.RygProviderActivity">
        </activity>

        <service
            android:name=".module.ryg.ch2.messenger.MessengerService"
            android:process=":remote">
            <intent-filter>
                <action android:name="com.tustar.MessengerService.launch"/>
            </intent-filter>
        </service>
        <service
            android:name=".module.ryg.ch2.aidl.BookManagerService"

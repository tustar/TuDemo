        <!-- FileManager demo -->
        <activity
            android:name=".module.filemanager.GifPlayerActivity"
            android:hardwareAccelerated="false"/>
        <activity android:name=".module.filemanager.AndroidGifDrawableActivity">
        </activity>

    </application>

</manifest>
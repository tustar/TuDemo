<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/launcher_home">

    <fragment
        android:id="@+id/launcher_home"
        android:name="com.tustar.demo.module.jet.ui.StartFragment"
        android:label="Home"
        tools:layout="@layout/fragment_jet_start">
        <action
            android:id="@+id/end_action"
            app:destination="@id/end_dest" />
    </fragment>

    <fragment
        android:id="@+id/end_dest"
        android:name="com.tustar.demo.module.jet.ui.EndFragment"
        android:label="End"
        tools:layout="@layout/fragment_jet_end" />
</navigation>
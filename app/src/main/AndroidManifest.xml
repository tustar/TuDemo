<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tustar.demo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.RecyclerViewActivity" />
        <activity
            android:name=".activity.RotationActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:screenOrientation="user"
            android:windowSoftInputMode="stateAlwaysHidden" />
        <activity
            android:name=".activity.PaletteActivity"
            android:label="@string/title_activity_palette" />
        <activity android:name=".activity.TintingActivity" />
        <activity android:name=".activity.ClippingActivity" />
        <activity android:name=".activity.ElevationActivity" />
        <activity android:name=".activity.AnimMainActivity" />
        <activity android:name=".activity.AnimTransitionsActivity" />
        <activity android:name=".activity.CustomWidgetActivity" />
        <activity android:name=".activity.CustomWidgetShowActivity"></activity>
    </application>

</manifest>
